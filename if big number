a=int(input())
b=int(input())
c=int(input())
d=int(input())
if (a>b):
    if (a>c):
        if (a>d):
            print("a is big")
if (b>a):
    if (b>c):
        if (b<d):
                  print("b is big")
if (c>a):
        if (c>d):
                if(c>b):
                  print("c is big")
if (d>a):
        if (d>b):
                  if (d>c):
                    print("d is big")
