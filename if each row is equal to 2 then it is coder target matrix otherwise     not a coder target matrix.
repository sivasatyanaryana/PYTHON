n=int(input("enter matrix size"))
m=[]
z=[]
a=[]
co=0
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
    m.append(l)
for i in range(0,n):
    sum=0
    for j in range(0,n):
        sum=sum+m[i][j]
    if sum==2:
        co=co+1
if (co==3):
    print("Coder target matrix")
else:
    print("not coder target matrix")
