s=input()
az=0
ds=0
lc=0
uc=0
for i in s:
    if i.isalpha():
        az=az+1
    if i.isdigit():
        ds=ds+1
    if i.isupper():
        uc=uc+1
    if i.islower():
        lc=lc+1
if az>0 or ds>0:
    print(True)
else:
    print(False)
if az>0:
    print(True)
else:
    print(False)
if ds>0:
    print(True)
else:
    print(False)
if lc>0:
    print(True)
else:
    print(False)
if uc>0:
    print(True)
else:
    print(False)
